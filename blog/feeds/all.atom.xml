&lt;span class="n"&gt;release&lt;/span&gt; &lt;span class="n"&gt;目錄取得&lt;/span&gt; &lt;span class="n"&gt;scarysim&lt;/span&gt;&lt;span class="mf"&gt;.&lt;/span&gt;&lt;span class="n"&gt;exe&lt;/span&gt;

&lt;span class="mf"&gt;6.&lt;/span&gt;&lt;span class="n"&gt;因為&lt;/span&gt; &lt;span class="n"&gt;scarysim&lt;/span&gt;&lt;span class="mf"&gt;.&lt;/span&gt;&lt;span class="n"&gt;exe&lt;/span&gt; &lt;span class="n"&gt;執行需要&lt;/span&gt; &lt;span class="n"&gt;base …&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;</summary><content type="html">&lt;p&gt;W7網站內容&lt;/p&gt;
